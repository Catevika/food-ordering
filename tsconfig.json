{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [ "src/*" ],
      "@/components/*": [ "src/components/*" ],
      "@/assets/*": [ "assets/*" ],
      "@/constants/*": [ "src/constants/*" ],
      "@/lib/*": [ "src/lib/*" ],
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/types.d.ts"
  ]
}
